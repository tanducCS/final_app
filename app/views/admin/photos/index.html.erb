
<div class="col-10 col-md-8 bg-body ">

  <!-- List items -->
  <div class="list-items row d-flex justify-content-evenly  mt-2 mb-3">
    <% @photos.each do |photo| %>
      <div class="list-items__item  mb-3 col-md-6 col-lg-3  d-flex justify-content-center align-items-center   " >
        <div class=" item__container-img rounded overflow-hidden d-flex justify-content-center align-items-center  shadow" style="width: 250px; height:190px">
          <%= image_tag photo.image_url.url,style:'width:100%' %>
            <div class="photo_admin container__edit_photo d-flex justify-content-between align-items-center bg-black w-100 ">
              <p class="text-white mx-3 d-inline-block text-truncate fs-5"><%= photo.title %></p>
              <%= link_to edit_photo_path(photo) do %>
                <i class="fa-solid fa-pen-to-square fa-xl me-1" style="color: #ffffff;"></i>
              <%end %>
            </div>

        </div>
      </div>
    <%end %>
  </div>

  <!-- Pagination -->
  <nav aria-label="" class="d-flex justify-content-center">
    <ul class="pagination">
      <li class="page-item disabled">
        <a class="page-link">Previous</a>
      </li>
      <li class="page-item active" ><a class="page-link" href="#">1</a></li>
      <li class="page-item " aria-current="page">
        <a class="page-link" href="#">2</a>
      </li>
      <li class="page-item"><a class="page-link" href="#">3</a></li>
      <li class="page-item">
        <a class="page-link" href="#">Next</a>
      </li>
    </ul>
  </nav>

</div>
<div class="col-sm-2 ">

</div>




